const counter = {
  count: 0,

  increment: function () {
    this.count++;
  },

  decrement: function () {
    this.count--;
  },

  reset: function () {
    this.count = 0;
  },

  getCount: function () {
    return this.count;
  }
};

counter.increment();
counter.increment();
console.log(counter.getCount()); 
counter.decrement();
console.log(counter.getCount()); 
counter.reset();
console.log(counter.getCount()); 
